<Window x:Class="LaberintoAStar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaberintoAStar"
        mc:Ignorable="d" Loaded="Window_Loaded"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Left">
            <Button x:Name="btnResolver" Click="btnResolver_Click">Resolver</Button>
        </StackPanel>
        <UniformGrid Width="400" Height="400" x:Name="gridTablero" Rows="20" Columns="20">
            
        </UniformGrid>
    </DockPanel>
</Window>
